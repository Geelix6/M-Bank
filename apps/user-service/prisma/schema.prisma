generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id
  username  String   @unique
  email     String   @unique
  firstName String
  lastName  String
  balance   Decimal
  createdAt DateTime @default(now())
}
